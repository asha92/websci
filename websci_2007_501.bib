@article{ ISI:000250759800023,
Author = {Elias, Daniel and Britton, Sven and Kassu, Afework and Akuffo, Hannah},
Title = {{Chronic helminth infections may negatively influence immunity against
   tuberculosis and other diseases of public health importance}},
Journal = {{EXPERT REVIEW OF ANTI-INFECTIVE THERAPY}},
