@article{ ISI:000249393200002,
Author = {Burrill, Joshua and Williams, Christopher J. and Bain, Gillian and
   Conder, Gabriel and Hine, Andrew L. and Misra, Rakesh R.},
Title = {{Tuberculosis: a radiologic review}},
